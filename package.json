{
  "name": "reglo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"DOTENV_CONFIG_PATH=.env.dev NODE_OPTIONS=--require=dotenv/config next dev\" \"DOTENV_CONFIG_PATH=.env.dev NODE_OPTIONS=--require=dotenv/config npx trigger.dev@latest dev\"",
    "build": "DOTENV_CONFIG_PATH=.env.dev NODE_OPTIONS=--require=dotenv/config next build",
    "build:prod": "DOTENV_CONFIG_PATH=.env.prod NODE_OPTIONS=--require=dotenv/config next build",
    "start": "concurrently \"DOTENV_CONFIG_PATH=.env.dev NODE_OPTIONS=--require=dotenv/config next start\" \"DOTENV_CONFIG_PATH=.env.dev NODE_OPTIONS=--require=dotenv/config npx trigger.dev@latest dev\"",
    "start:prod": "DOTENV_CONFIG_PATH=.env.prod NODE_OPTIONS=--require=dotenv/config next start",
    "lint": "next lint",
    "postinstall": "npx prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "trigger:dev": "DOTENV_CONFIG_PATH=.env.dev NODE_OPTIONS=--require=dotenv/config npx trigger.dev@latest dev",
    "trigger:deploy:dev": "DOTENV_CONFIG_PATH=.env.dev NODE_OPTIONS=--require=dotenv/config npx trigger.dev@latest deploy",
    "trigger:deploy:prod": "DOTENV_CONFIG_PATH=.env.prod NODE_OPTIONS=--require=dotenv/config npx trigger.dev@latest deploy",
    "migrate:dev": "DOTENV_CONFIG_PATH=.env.dev NODE_OPTIONS=--require=dotenv/config prisma migrate dev",
    "migrate:prod": "DOTENV_CONFIG_PATH=.env.prod NODE_OPTIONS=--require=dotenv/config prisma migrate deploy",
    "studio:dev": "DOTENV_CONFIG_PATH=.env.dev NODE_OPTIONS=--require=dotenv/config prisma studio",
    "studio:prod": "DOTENV_CONFIG_PATH=.env.prod NODE_OPTIONS=--require=dotenv/config prisma studio",
    "email": "cp .env ./node_modules/react-email && email dev --dir email --port 3001"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@aws-sdk/client-s3": "^3.693.0",
    "@aws-sdk/s3-request-presigner": "^3.693.0",
    "@hookform/resolvers": "^3.9.1",
    "@neondatabase/serverless": "^0.10.3",
    "@notionhq/client": "^5.6.0",
    "prisma": "^6.11.1",
    "@prisma/adapter-neon": "^6.11.1",
    "@prisma/client": "^6.11.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
