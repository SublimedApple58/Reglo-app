    "bufferutil": "^4.0.8",
    "concurrently": "^9.2.1",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "jest": "^29.7.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
