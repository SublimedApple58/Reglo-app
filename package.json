    "make-cancellable-promise": "^2.0.0",
    "make-event-props": "^2.0.0",
    "merge-refs": "^2.0.0",
    "motion": "^12.15.0",
    "next": "^15.2.2",
    "next-auth": "^5.0.0-beta.25",
    "next-intl": "^4.1.0",
    "next-themes": "^0.4.3",
    "nextstepjs": "^2.1.1",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.4.168",
    "preact": "^10.28.1",
    "query-string": "^9.1.1",
    "radix-ui": "^1.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-email": "^3.0.4",
    "react-hook-form": "^7.53.2",
    "react-pdf": "^9.1.0",
    "react-use-measure": "^2.1.7",
    "reactflow": "^11.11.4",
    "recharts": "^2.14.1",
    "resend": "^4.0.1",
    "slugify": "^1.6.6",
