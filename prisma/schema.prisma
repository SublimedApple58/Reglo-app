generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
  binaryTargets   = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

enum IntegrationProvider {
  SLACK
  FATTURE_IN_CLOUD
}

enum ServiceKey {
  DOC_MANAGER
  WORKFLOWS
  AI_ASSISTANT
  AUTOSCUOLE
}

enum CompanyServiceStatus {
  ACTIVE
  DISABLED
}

enum AutoscuolaRole {
  OWNER
  INSTRUCTOR
  STUDENT
}

model User {
  id            String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name          String    @default("NO_NAME")
  email         String    @unique(map: "user_email_idx")
  emailVerified DateTime? @db.Timestamp(6)
  image         String?
  password      String?
  role          String    @default("user")
  activeCompanyId String? @db.Uuid
  address       Json?     @db.Json
  paymentMethod String?
  createdAt     DateTime  @default(now()) @db.Timestamp(6)
  updatedAt     DateTime  @updatedAt
  account       Account[]
  session       Session[]
  Order         Order[]
  activeCompany Company?  @relation("UserActiveCompany", fields: [activeCompanyId], references: [id], onDelete: SetNull)
  companyMembers CompanyMember[]
  companyInvites CompanyInvite[] @relation("CompanyInviteInvitedBy")
  documentRequests DocumentRequest[] @relation("DocumentRequestCreatedBy")
  mobileTokens MobileAccessToken[]
  autoscuolaInstructors AutoscuolaInstructor[]
}

model Account {
  userId            String  @db.Uuid
  type              String
  provider          String
  providerAccountId String
  refresh_token     String?
  access_token      String?
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String?
  session_state     String?

  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  user User @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@id([provider, providerAccountId])
}

model Session {
  sessionToken String   @id
  userId       String   @db.Uuid
  expires      DateTime @db.Timestamp(6)
  user         User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt
}

model VerificationToken {
  identifier String
  token      String
  expires    DateTime

  @@id([identifier, token])
}

model MobileAccessToken {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  userId    String   @db.Uuid
  companyId String?  @db.Uuid
  tokenHash String   @unique
  createdAt DateTime @default(now()) @db.Timestamp(6)
  lastUsedAt DateTime? @db.Timestamp(6)
  expiresAt DateTime @db.Timestamp(6)

  user     User    @relation(fields: [userId], references: [id], onDelete: Cascade)
  company  Company? @relation(fields: [companyId], references: [id], onDelete: SetNull)

  @@index([userId])
  @@index([companyId])
  @@index([expiresAt])
}

model Order {
  id              String      @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  userId          String      @db.Uuid
  shippingAddress Json        @db.Json
  paymentMethod   String
  paymentResult   Json?       @db.Json
  itemsPrice      Decimal     @db.Decimal(12, 2)
  shippingPrice   Decimal     @db.Decimal(12, 2)
  taxPrice        Decimal     @db.Decimal(12, 2)
  totalPrice      Decimal     @db.Decimal(12, 2)
  isPaid          Boolean     @default(false)
  paidAt          DateTime?   @db.Timestamp(6)
  isDelivered     Boolean     @default(false)
  deliveredAt     DateTime?   @db.Timestamp(6)
  createdAt       DateTime    @default(now()) @db.Timestamp(6)
  user            User        @relation(fields: [userId], references: [id], onDelete: Cascade)
  orderitems      OrderItem[]
}

model OrderItem {
  orderId   String  @db.Uuid
  productId String  @db.Uuid
  qty       Int
  price     Decimal @db.Decimal(12, 2)
  name      String
  slug      String
  image     String
  order     Order   @relation(fields: [orderId], references: [id], onDelete: Cascade)

  @@id([orderId, productId], map: "orderitems_orderId_productId_pk")
}

model Company {
  id        String           @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name      String
  logoKey   String?
  createdAt DateTime         @default(now()) @db.Timestamp(6)
  updatedAt DateTime         @updatedAt

  members   CompanyMember[]
  activeUsers User[] @relation("UserActiveCompany")
  documents DocumentTemplate[]
  invites   CompanyInvite[]
  documentRequests DocumentRequest[]
  workflows Workflow[]
  workflowRuns WorkflowRun[]
  integrationConnections IntegrationConnection[]
  autoscuolaStudents AutoscuolaStudent[]
  autoscuolaCases AutoscuolaCase[]
  autoscuolaAppointments AutoscuolaAppointment[]
  autoscuolaInstructors AutoscuolaInstructor[]
  autoscuolaVehicles AutoscuolaVehicle[]
  autoscuolaAvailabilitySlots AutoscuolaAvailabilitySlot[]
  autoscuolaWeeklyAvailabilities AutoscuolaWeeklyAvailability[]
  autoscuolaBookingRequests AutoscuolaBookingRequest[]
  autoscuolaWaitlistOffers AutoscuolaWaitlistOffer[]
  autoscuolaDocuments AutoscuolaDocument[]
  autoscuolaPaymentPlans AutoscuolaPaymentPlan[]
  autoscuolaMessageTemplates AutoscuolaMessageTemplate[]
  autoscuolaMessageRules AutoscuolaMessageRule[]
  autoscuolaMessageLogs AutoscuolaMessageLog[]
  services CompanyService[]
  mobileTokens MobileAccessToken[]
}

model CompanyService {
  id        String               @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String               @db.Uuid
  serviceKey ServiceKey
  status    CompanyServiceStatus @default(ACTIVE)
  limits    Json?                @db.Json
  createdAt DateTime             @default(now()) @db.Timestamp(6)
  updatedAt DateTime             @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)

  @@unique([companyId, serviceKey])
  @@index([companyId])
  @@index([serviceKey])
}

model CompanyMember {
  companyId String   @db.Uuid
  userId    String   @db.Uuid
  role      String   @default("member")
  autoscuolaRole AutoscuolaRole @default(STUDENT)
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  user      User    @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@id([companyId, userId])
  @@index([userId])
}

model CompanyInvite {
  id          String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId   String   @db.Uuid
  email       String
  role        String   @default("member")
  token       String   @unique
  status      String   @default("pending")
  expiresAt   DateTime @db.Timestamp(6)
  invitedById String?  @db.Uuid
  createdAt   DateTime @default(now()) @db.Timestamp(6)
  updatedAt   DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  invitedBy User?   @relation("CompanyInviteInvitedBy", fields: [invitedById], references: [id], onDelete: SetNull)

  @@index([companyId])
  @@index([email])
}

model IntegrationConnection {
  id        String              @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String              @db.Uuid
  provider  IntegrationProvider
  status    String              @default("connected")
  externalAccountId String?
  displayName String?
  scope     String?
  accessTokenCiphertext String?
  accessTokenIv String?
  accessTokenTag String?
  refreshTokenCiphertext String?
  refreshTokenIv String?
  refreshTokenTag String?
  expiresAt DateTime?
  metadata  Json?
  createdAt DateTime            @default(now()) @db.Timestamp(6)
  updatedAt DateTime            @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)

  @@unique([companyId, provider])
  @@index([provider])
}

model DocumentTemplate {
  id        String          @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String          @db.Uuid
  name      String
  sourceUrl String?
  createdAt DateTime        @default(now()) @db.Timestamp(6)
  updatedAt DateTime        @updatedAt

  company   Company        @relation(fields: [companyId], references: [id], onDelete: Cascade)
  fields    DocumentField[]
  requests  DocumentRequest[]

  @@index([companyId])
}

model DocumentField {
  id         String           @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  templateId String           @db.Uuid
  type       String
  label      String?
  bindingKey String?
  page       Int              @default(1)
  x          Float
  y          Float
  width      Float
  height     Float
  meta       Json?
  createdAt  DateTime         @default(now()) @db.Timestamp(6)
  updatedAt  DateTime         @updatedAt

  template   DocumentTemplate @relation(fields: [templateId], references: [id], onDelete: Cascade)

  @@index([templateId])
}

model DocumentRequest {
  id              String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId       String   @db.Uuid
  templateId      String   @db.Uuid
  name            String
  status          String   @default("in_progress")
  publicToken     String   @unique
  payload         Json?
  resultUrl       String?
  completedByName String?
  completedAt     DateTime? @db.Timestamp(6)
  createdById     String?  @db.Uuid
  createdAt       DateTime @default(now()) @db.Timestamp(6)
  updatedAt       DateTime @updatedAt

  company   Company          @relation(fields: [companyId], references: [id], onDelete: Cascade)
  template  DocumentTemplate @relation(fields: [templateId], references: [id], onDelete: Cascade)
  createdBy User?            @relation("DocumentRequestCreatedBy", fields: [createdById], references: [id], onDelete: SetNull)

  @@index([companyId])
  @@index([templateId])
}

model Workflow {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  name      String
  status    String   @default("draft")
  definition Json    @db.Json
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  runs      WorkflowRun[]

  @@index([companyId])
  @@index([status])
}

model WorkflowRun {
  id            String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  workflowId    String   @db.Uuid
  companyId     String   @db.Uuid
  status        String   @default("queued")
  triggerType   String
  triggerPayload Json?   @db.Json
  startedAt     DateTime? @db.Timestamp(6)
  finishedAt    DateTime? @db.Timestamp(6)
  createdAt     DateTime  @default(now()) @db.Timestamp(6)

  workflow Workflow @relation(fields: [workflowId], references: [id], onDelete: Cascade)
  company  Company  @relation(fields: [companyId], references: [id], onDelete: Cascade)
  steps    WorkflowRunStep[]

  @@index([workflowId])
  @@index([companyId])
  @@index([status])
}

model WorkflowRunStep {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  runId     String   @db.Uuid
  nodeId    String
  status    String   @default("pending")
  input     Json?    @db.Json
  output    Json?    @db.Json
  error     Json?    @db.Json
  attempt   Int      @default(0)
  startedAt DateTime? @db.Timestamp(6)
  finishedAt DateTime? @db.Timestamp(6)
  createdAt DateTime  @default(now()) @db.Timestamp(6)

  run WorkflowRun @relation(fields: [runId], references: [id], onDelete: Cascade)

  @@index([runId])
  @@index([status])
}

model AutoscuolaStudent {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  firstName String
  lastName  String
  email     String?
  phone     String?
  status    String   @default("active")
  notes     String?
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  cases     AutoscuolaCase[]
  appointments AutoscuolaAppointment[]
  documents AutoscuolaDocument[]
  paymentPlans AutoscuolaPaymentPlan[]
  messageLogs AutoscuolaMessageLog[]
  bookingRequests AutoscuolaBookingRequest[]
  waitlistResponses AutoscuolaWaitlistResponse[]

  @@index([companyId])
  @@index([status])
  @@index([lastName, firstName])
}

model AutoscuolaCase {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  studentId String   @db.Uuid
  category  String?
  status    String   @default("iscritto")
  theoryExamAt DateTime? @db.Timestamp(6)
  drivingExamAt DateTime? @db.Timestamp(6)
  pinkSheetExpiresAt DateTime? @db.Timestamp(6)
  medicalExpiresAt DateTime? @db.Timestamp(6)
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  student   AutoscuolaStudent @relation(fields: [studentId], references: [id], onDelete: Cascade)
  appointments AutoscuolaAppointment[]
  paymentPlans AutoscuolaPaymentPlan[]

  @@index([companyId])
  @@index([studentId])
  @@index([status])
}

model AutoscuolaAppointment {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  studentId String   @db.Uuid
  caseId    String?  @db.Uuid
  slotId    String?  @db.Uuid
  type      String
  startsAt  DateTime @db.Timestamp(6)
  endsAt    DateTime? @db.Timestamp(6)
  status    String   @default("scheduled")
  instructorId String? @db.Uuid
  vehicleId String? @db.Uuid
  notes     String?
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  student   AutoscuolaStudent @relation(fields: [studentId], references: [id], onDelete: Cascade)
  case      AutoscuolaCase? @relation(fields: [caseId], references: [id], onDelete: SetNull)
  slot      AutoscuolaAvailabilitySlot? @relation(fields: [slotId], references: [id], onDelete: SetNull)
  instructor AutoscuolaInstructor? @relation(fields: [instructorId], references: [id], onDelete: SetNull)
  vehicle    AutoscuolaVehicle? @relation(fields: [vehicleId], references: [id], onDelete: SetNull)
  messageLogs AutoscuolaMessageLog[]

  @@index([companyId])
  @@index([studentId])
  @@index([startsAt])
  @@index([instructorId])
  @@index([vehicleId])
  @@index([slotId])
}

model AutoscuolaInstructor {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  userId    String?  @db.Uuid
  name      String
  phone     String?
  status    String   @default("active")
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  user      User?   @relation(fields: [userId], references: [id], onDelete: SetNull)
  appointments AutoscuolaAppointment[]

  @@index([companyId])
  @@index([userId])
  @@unique([companyId, userId])
}

model AutoscuolaVehicle {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  name      String
  plate     String?
  status    String   @default("active")
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  appointments AutoscuolaAppointment[]

  @@index([companyId])
}

model AutoscuolaWeeklyAvailability {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  ownerType String
  ownerId   String   @db.Uuid
  daysOfWeek Int[]
  startMinutes Int
  endMinutes Int
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)

  @@index([companyId])
  @@index([ownerType, ownerId])
  @@unique([companyId, ownerType, ownerId])
}

model AutoscuolaAvailabilitySlot {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  ownerType String
  ownerId   String   @db.Uuid
  startsAt  DateTime @db.Timestamp(6)
  endsAt    DateTime @db.Timestamp(6)
  status    String   @default("open")
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  appointments AutoscuolaAppointment[]
  waitlistOffers AutoscuolaWaitlistOffer[]

  @@index([companyId])
  @@index([ownerType, ownerId])
  @@index([startsAt])
  @@unique([companyId, ownerType, ownerId, startsAt])
}

model AutoscuolaBookingRequest {
  id          String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId   String   @db.Uuid
  studentId   String   @db.Uuid
  desiredDate DateTime @db.Timestamp(6)
  status      String   @default("pending")
  createdAt   DateTime @default(now()) @db.Timestamp(6)
  updatedAt   DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  student   AutoscuolaStudent @relation(fields: [studentId], references: [id], onDelete: Cascade)

  @@index([companyId])
  @@index([studentId])
  @@index([desiredDate])
}

model AutoscuolaWaitlistOffer {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  slotId    String   @db.Uuid
  status    String   @default("broadcasted")
  sentAt    DateTime @db.Timestamp(6)
  expiresAt DateTime @db.Timestamp(6)
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  slot      AutoscuolaAvailabilitySlot @relation(fields: [slotId], references: [id], onDelete: Cascade)
  responses AutoscuolaWaitlistResponse[]

  @@index([companyId])
  @@index([slotId])
}

model AutoscuolaWaitlistResponse {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  offerId   String   @db.Uuid
  studentId String   @db.Uuid
  status    String   @default("accepted")
  respondedAt DateTime @db.Timestamp(6)
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  offer     AutoscuolaWaitlistOffer @relation(fields: [offerId], references: [id], onDelete: Cascade)
  student   AutoscuolaStudent @relation(fields: [studentId], references: [id], onDelete: Cascade)

  @@index([offerId])
  @@index([studentId])
}

model AutoscuolaDocument {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  studentId String   @db.Uuid
  name      String
  status    String   @default("missing")
  fileUrl   String?
  expiresAt DateTime? @db.Timestamp(6)
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  student   AutoscuolaStudent @relation(fields: [studentId], references: [id], onDelete: Cascade)

  @@index([companyId])
  @@index([studentId])
}

model AutoscuolaPaymentPlan {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  studentId String   @db.Uuid
  caseId    String?  @db.Uuid
  totalAmount Decimal @db.Decimal(12, 2)
  currency  String   @default("EUR")
  status    String   @default("active")
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  student   AutoscuolaStudent @relation(fields: [studentId], references: [id], onDelete: Cascade)
  case      AutoscuolaCase? @relation(fields: [caseId], references: [id], onDelete: SetNull)
  installments AutoscuolaPaymentInstallment[]

  @@index([companyId])
  @@index([studentId])
}

model AutoscuolaPaymentInstallment {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  planId    String   @db.Uuid
  dueDate   DateTime @db.Timestamp(6)
  amount    Decimal  @db.Decimal(12, 2)
  status    String   @default("pending")
  paidAt    DateTime? @db.Timestamp(6)
  invoiceId String?
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  plan      AutoscuolaPaymentPlan @relation(fields: [planId], references: [id], onDelete: Cascade)

  @@index([planId])
  @@index([dueDate])
}

model AutoscuolaMessageTemplate {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  name      String
  channel   String
  subject   String?
  body      String
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  rules     AutoscuolaMessageRule[]
  logs      AutoscuolaMessageLog[]

  @@index([companyId])
}

model AutoscuolaMessageRule {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId String   @db.Uuid
  templateId String  @db.Uuid
  type      String   // APPOINTMENT_BEFORE, CASE_STATUS_CHANGED
  appointmentType String? // guida, esame
  deadlineType String? // PINK_SHEET_EXPIRES, MEDICAL_EXPIRES
  offsetDays Int     @default(0)
  channel   String   // email, sms
  target    String   // student, staff
  active    Boolean  @default(true)
  createdAt DateTime @default(now()) @db.Timestamp(6)
  updatedAt DateTime @updatedAt

  company   Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  template  AutoscuolaMessageTemplate @relation(fields: [templateId], references: [id], onDelete: Cascade)
  logs      AutoscuolaMessageLog[]

  @@index([companyId])
  @@index([type])
}

model AutoscuolaMessageLog {
  id          String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  companyId   String   @db.Uuid
  ruleId      String   @db.Uuid
  templateId  String   @db.Uuid
  appointmentId String? @db.Uuid
  studentId   String?  @db.Uuid
  channel     String
  recipient   String
  status      String   @default("sent")
  error       String?
  payload     Json?
  sentAt      DateTime @default(now()) @db.Timestamp(6)

  company     Company @relation(fields: [companyId], references: [id], onDelete: Cascade)
  rule        AutoscuolaMessageRule @relation(fields: [ruleId], references: [id], onDelete: Cascade)
  template    AutoscuolaMessageTemplate @relation(fields: [templateId], references: [id], onDelete: Cascade)
  appointment AutoscuolaAppointment? @relation(fields: [appointmentId], references: [id], onDelete: SetNull)
  student     AutoscuolaStudent? @relation(fields: [studentId], references: [id], onDelete: SetNull)

  @@index([companyId])
  @@index([ruleId])
  @@index([appointmentId])
  @@unique([ruleId, appointmentId, recipient, channel])
}
